{
  "manifest_version": 3,
  "name": "NJU 自动登录助手",
  "version": "2.5",
  "permissions": ["storage", "activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://authserver.nju.edu.cn/*"],
      "js": ["tesseract.min.js", "content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["langs/*"],
      "matches": ["<all_urls>"]
    }
  ]
}